# meepy-moopy SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: PETSTORE_
#
# Security environment variables:
PETSTORE_API_KEY=your_api_key_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
PETSTORE_DEBUG=false
